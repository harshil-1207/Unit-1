#include <stdio.h>
int main() {
int num, square, temp, digits = 0, last;
printf("Enter a number: ");
scanf("%d", &num);
square = num * num;
temp = num;
while(temp > 0) {
digits++;
temp = temp / 10;
}
last = square % (int)pow(10, digits);
if(last == num) {
printf("%d is an automorphic number\n", num);
} else {
printf("%d is not an automorphic number\n", num);
}
return 0;
}
